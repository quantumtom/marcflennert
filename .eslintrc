{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended"],
  "globals": {
    "chrome": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "none",
        "singleQuote": true
      }
    ],
    "no-console": "off"
  },
  "plugins": ["jquery", "babel", "prettier", "eslint-plugin-jquery"]
}
